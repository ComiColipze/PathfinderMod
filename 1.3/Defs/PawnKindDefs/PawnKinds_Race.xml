    <techHediffsTags>
		<li>Poor</li>
    </techHediffsTags>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <invNutrition>1</invNutrition>
    <invFoodDef>Pemmican</invFoodDef>
    <initialWillRange>0~0</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>
