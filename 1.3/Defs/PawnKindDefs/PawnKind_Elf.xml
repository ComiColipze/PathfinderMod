<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<PawnKindDef ParentName="BasePlayerPawnKind">
		<defName>PF2_ElfColonist</defName>
		<label>elf colonist</label>
		<race>PF2_Elf</race>
		<combatPower>30</combatPower>
		<isFighter>false</isFighter>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<forceNormalGearQuality>true</forceNormalGearQuality>
		<initialWillRange>0~0</initialWillRange>
		<defaultFactionType>PF2_ElfPlayerColony</defaultFactionType>
		<chemicalAddictionChance>0.06</chemicalAddictionChance>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>Outlander</li>
					<li>Elven</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<apparelTags>
			<li>IndustrialBasic</li>
		</apparelTags>
		<apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
		<apparelMoney>350~600</apparelMoney>
		<backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
		<techHediffsChance>0.03</techHediffsChance>
		<techHediffsMoney>50~800</techHediffsMoney>
		<techHediffsTags>
			<li>Poor</li>
			<li>Simple</li>
			<li>ImplantEmpireCommon</li>
		</techHediffsTags>
		<initialResistanceRange>13~21</initialResistanceRange>
	</PawnKindDef>

  	<PawnKindDef ParentName="BasePlayerPawnKind">
		<defName>PF2_ElfTribal</defName>
		<label>tribal elf</label>
		<race>PF2_Elf</race>
		<defaultFactionType>PF2_ElfPlayerTribe</defaultFactionType>
		<chemicalAddictionChance>0.05</chemicalAddictionChance>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>Tribal</li>
					<li>Elven</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<apparelTags>
			<li>Neolithic</li>
		</apparelTags>
		<apparelMoney>200~200</apparelMoney>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<techHediffsMoney>50~50</techHediffsMoney>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
		<techHediffsChance>0.02</techHediffsChance>
		<initialResistanceRange>9~15</initialResistanceRange>
	</PawnKindDef>

	<PawnKindDef ParentName="BasePlayerPawnKind">
		<defName>PF2_ElfLiegeman</defName>
		<label>elf liegeman</label>
		<race>PF2_Elf</race>
		<defaultFactionType>PF2_ElfPlayerFaction</defaultFactionType>
		<chemicalAddictionChance>0.05</chemicalAddictionChance>
		<apparelTags>
			<li>MedievalBasic</li>
			<li>Neolithic</li>
		</apparelTags>
		<apparelMoney>200~200</apparelMoney>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<techHediffsMoney>500~500</techHediffsMoney>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
		<techHediffsChance>0.02</techHediffsChance>
		<initialResistanceRange>12~17</initialResistanceRange>
	 </PawnKindDef>

</Defs>