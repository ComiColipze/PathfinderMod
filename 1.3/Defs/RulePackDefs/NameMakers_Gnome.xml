	<RulePackDef>
		<defName>PF2_LeaderTitleMaker_FirstWorlder</defName>
		<include>
			<li>LeaderTitleMaker_GlobalStructure</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>memeLeaderNoun(priority=1)->[cultureLeaderNoun]</li>

				<li>cultureLeaderTitle->[cultureLeaderAdjective] [cultureLeaderNoun]</li>

				<li>cultureLeaderAdjective->smart</li>
				<li>cultureLeaderAdjective->genius</li>
				<li>cultureLeaderAdjective->ingenious</li>

				<li>cultureLeaderNoun->emissary</li>
				<li>cultureLeaderNoun->jack-of-all-trades</li>
				<li>cultureLeaderNoun->councilor</li>
				<li>cultureLeaderNoun->leader</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
		<defName>PF2_NamerFactionGnomish</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->The [PoliticalUnionsMedieval] of [GnomeWelsh1][GnomeWelsh2]</li>
				<li>r_name->The [PoliticalUnionsMedieval] of [Gnome6]</li>
				<li>r_name->[GnomeWelsh1][GnomeWelsh2] [PoliticalUnionsMedieval]</li>
				<li>r_name->[Gnome6] [PoliticalUnionsMedieval]</li>
				<li>r_name->The [PoliticalUnionsMedieval] of First World Expatriates</li>
				<li>r_name->The [PoliticalUnionsMedieval] of First World Refugees</li>
			</rulesStrings>
			<rulesFiles>
				<li>GnomeWelsh1->GnomeNames/GnomeWelsh1</li>
				<li>GnomeWelsh2->GnomeNames/GnomeWelsh2</li>
				<li>Gnome6->GnomeNames/Gnome6</li>
				<li>PoliticalUnionsMedieval->PF2Factions/PoliticalUnionsMedieval</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>PF2_NamerSettlementGnomish</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->[townname_wordgen]</li>

				<li>townname_wordgen(p=2)->[Gnome6]</li>
				<li>townname_wordgen(p=2)->[SylE][SylE][SylE]stan</li>
				<li>townname_wordgen(p=2)->[SylE][SylE][SylE]fast</li>
