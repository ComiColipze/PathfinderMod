<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <MemeDef>
    <defName>PF2_Structure_OriginInnerSea</defName>
    <label>Inner Sea origin</label>
    <description>This ideoligion is a descendant of the ancient Inner Sea pantheon of deities. It may have very different precepts than those of thousands of years ago, but its symbols are similar.</description>
    <category>Structure</category>
    <iconPath>UI/Structures/Structure_InnerSea</iconPath>
    <impact>0</impact>
    <worshipRoomLabel>church</worshipRoomLabel>
    <deityCount>1~4</deityCount>
    <generalRules>
      <rulesStrings>
        <li>memeCreed->sect</li>
        <li>memeCreed->cult</li>
        <li>memeCreed->church</li>
        <li>memeCreed->faith</li>
        <li>memeCreed->pantheon</li>
        <li>memeHyphenPrefix->theo</li>
        <li>memeHyphenPrefix->inner</li>
        <li>memeConcept->creation</li>
        <li>memeConcept->life</li>
        <li>memeLeaderNoun->champion</li>
        <li>memeLeaderNoun->paladin</li>
        <li>memeLeaderAdjective->divine</li>
        <li>memeLeaderAdjective->clerical</li>
        <li>memeLeaderAdjective->priestly</li>
        <li>memeMoralist->priest</li>
        <li>memeMoralist->cleric</li>
      </rulesStrings>
    </generalRules>
    <descriptionMaker>
      <patterns>
        <li><def>TheistMyth</def></li>
        <li><def>TheistFounder</def></li>
      </patterns>
      <constants>
        <li><key>structure_originInnerSea</key><value>true</value></li>
      </constants>
    </descriptionMaker>
